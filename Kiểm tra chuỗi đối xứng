 So can tinh  <input type="text" id="num"/>
<input  type="button"  value="Enter" onclick="Kt(chuoi)"  "/>
<script>
function Kt(chuoi){
    chuoi=trim(chuoi);
    for ( i = 0 , j = chuoi.length-1;i<j;i++;j--)
    {
        if ( chuoi.substr(i,1)!= chuoi.substr(j,1))
            return " False "
    }
    return " true "
}
    
</script>
