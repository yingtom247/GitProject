<script>
var Temp = function(temp){
    this.getTemp() = function(){
        return temp ;
    }
    this.changeTempCF()=function(){
        var newT = temp * 3.28 ;
        return newT ;
    }
}
var tem = new Temp(30);
var te = tem.getTemp();
var cf = tem.changeTempCF();
alert ( cf);
alert(te);
</script>

